#include <iostream>
#include<math.h>


using namespace std;

int main()
{
    int i=2,n,flag=0;
    cout<<"Enter n"<<endl;
    cin>>n;
    while(i<=sqrt(n))
    {
        if(n%i==0)
        {
            flag=1;
            break;
        }
        i++;

    }
    if(flag==0)
        {
            cout<<"Prime";
        }
        else
        {
            cout<<"Not Prime";
        }
    
    return 0;
}
